#include<stdio.h>
#include<string.h>

int main()
{
	char a[50];
	int i;
	int up = 0;
	int low = 0;
	gets(a);

	for (i = 0; i<strlen(a) ; i++)
	{
		if (a[i] >= 65 && a[i] <= 90)
			up++;
		if (a[i] >= 97 && a[i] <= 122)
			low++;	
	}

	printf("Uppercase : %d\n", up);
	printf("Lowercase : %d", low);
	return 0;
}
